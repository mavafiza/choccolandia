{
  "name": "chocolandia",
  "version": "1.0.0",
  "description": "Landing page para un emprendimiento familiar de chocotejas, bombonería y postres artesanales ubicado en Mercado Lima, Perú.",
  "scripts": {
    "build": "postcss css/style.postcss -o css/style.css",
    "watch:css": "postcss css/style.postcss -o css/style.css --watch",
    "serve": "live-server",
    "dev": "npm-run-all --parallel watch:css serve"
  },
  "keywords": [
    "artesanal",
    "chocotejas",
    "bomboneria",
    "lima",
    "peru",
    "landing-page"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1"
  }
}
